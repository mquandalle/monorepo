{
  "name": "isomorphic-git",
  "version": "1.25.7-inlang",
  "description": "A pure JavaScript reimplementation of git for node and browsers",
  "type": "module",
  "typings": "./index.d.ts",
  "main": "./index.cjs",
  "module": "./index.js",
  "unpkg": "./index.umd.min.js",
  "bin": {
    "isogit": "./cli.cjs"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isomorphic-git/isomorphic-git.git"
  },
  "keywords": [
    "git",
    "isomorphic"
  ],
  "author": "William Hilton <wmhilton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/isomorphic-git/isomorphic-git/issues"
  },
  "homepage": "https://isomorphic-git.org/",
  "files": [
    "cli.cjs",
    "browser-tests.json",
    "http/*",
    "index.cjs",
    "index.d.ts",
    "index.js",
    "index.umd.min.js",
    "index.umd.min.d.ts",
    "index.umd.min.js.map",
    "size_report.html"
  ],
  "prettier": "./.prettierrc.cjs",
  "bundlewatch": {
    "files": [
      {
        "path": "./index.umd.min.js",
        "maxSize": "100kb"
      }
    ],
    "ci": {
      "trackBranches": [
        "main"
      ]
    }
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/isomorphic-git",
    "logo": "https://opencollective.com/isomorphic-git/logo.txt"
  },
  "dependencies": {
    "async-lock": "^1.4.1",
    "clean-git-ref": "^2.0.1",
    "crc-32": "^1.2.2",
    "diff3": "^0.0.4",
    "ignore": "^5.3.1",
    "pako": "^1.0.11",
    "pify": "^5.0.0",
    "sha.js": "^2.4.11"
  }
}
